<Application 
  x:Class="CAS.CommServer.UAOOI.ConfigurationEditor.HostingApplication"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CAS.CommServer.UAOOI.ConfigurationEditor"
             mc:Ignorable="d" 
             >
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Resources/TabItemResource.xaml"/>
        <ResourceDictionary Source="Resources/WindowRegionResource.xaml"/>
        <ResourceDictionary Source="Resources/Styles.xaml"/>
        <ResourceDictionary Source="Simple Styles.xaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
